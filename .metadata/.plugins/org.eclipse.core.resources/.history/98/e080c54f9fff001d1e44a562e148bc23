	public static void main(String[] args) {
		Arraylist<String> list=new Arraylist<String>();
		list.add("one");
		list.add("two");
		list.add("three");
		System.out.println(list);
		list.add(1,"six");
		list.add(2,"seven");
		System.out.println(list);
		list.remove(3);
		System.out.println(list);
		
		
		
		
		
